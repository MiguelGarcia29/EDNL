	node [shape=circle, style=filled, fillcolor="#FFFFFF", fontcolor=black, fontsize=16, penwidth=3];
	edge [color="#36454F", fontcolor=black, fontsize=16, style=solid, penwidth=2.5];
	0;
	1;
	2;
	3;
	4;
	5;
	6;
	7;
	0 -- 1[label="8"];
	0 -- 4[label="10"];
	1 -- 2[label="5"];
	1 -- 6[label="10"];
	2 -- 3[label="2"];
	2 -- 7[label="11"];
	3 -- 5[label="10"];
}